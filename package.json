{
  "name": "Ding",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "cross-env APP_ENV=dev electron-vite dev",
    "build": "electron-vite build",
    "build:test": "cross-env APP_ENV=test electron-vite build",
    "build:prod": "cross-env APP_ENV=prod electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir --config electron-builder.prod.yml",
    "build:win": "npm run build && electron-builder --win --config electron-builder.prod.yml",
    "build:mac": "npm run build && electron-builder --mac --config electron-builder.prod.yml",
    "build:linux": "npm run build && electron-builder --linux --config electron-builder.prod.yml",
    "build:test:win": "cross-env APP_ENV=test npm run build:test && electron-builder --win --config electron-builder.test.yml",
    "build:test:mac": "cross-env APP_ENV=test npm run build:test && electron-builder --mac --config electron-builder.test.yml",
    "build:test:linux": "cross-env APP_ENV=test npm run build:test && electron-builder --linux --config electron-builder.test.yml",
    "build:prod:win": "cross-env APP_ENV=prod npm run build:prod && electron-builder --win --config electron-builder.prod.yml",
    "build:prod:mac": "cross-env APP_ENV=prod npm run build:prod && electron-builder --mac --config electron-builder.prod.yml",
    "build:prod:linux": "cross-env APP_ENV=prod npm run build:prod && electron-builder --linux --config electron-builder.prod.yml",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
